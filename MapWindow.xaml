<Window x:Name="mapWindow"  x:Class="TarkovAssistantWPF.MapWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TarkovAssistantWPF"
        mc:Ignorable="d"
        Title="Tarkov Assistant - Map Viewer" Height="768" Width="1366"  KeyDown="OnKeyDown" KeyUp="OnKeyUp" MouseWheel="OnMouseWheel">
    <Grid>
        <Image x:Name="pictureBox" Margin="0,10,0,0" MouseMove="OnMouseMove" MouseLeftButtonDown="OnMouseDown" PreviewMouseUp="OnMouseUp" OpacityMask="Black" Source="maps/customs.png">
            <Image.RenderTransform>
                <TransformGroup>
                    <MatrixTransform x:Name="mapTransform"/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
    </Grid>
</Window>
